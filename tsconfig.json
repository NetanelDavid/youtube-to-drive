{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"sourceMap": true,
		"declaration": true,
		"strict": true,
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"types": [
			"node",
		],
	}
}